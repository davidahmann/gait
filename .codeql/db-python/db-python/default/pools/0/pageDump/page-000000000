name: b ... n_id }}perf:name: perf-nightly/Users/davidahmann/Projects/gait/.github/workflows/release.ymltagsv*"v*"- "v*"tags:GORELEASER_VERSIONv2.7.0SYFT_VERSIONv1.32.0GRYPE_VERSIONv0.99.1COSIGN_VERSIONv2.5.3GORELEA ...  v2.7.0contents: writefetch-depthfetch-depth: 0Add Go bin to PATHecho "$(go env GOPATH)/bin" >> "$GITHUB_PATH"
name: A ... to PATHInstall goreleasergo install github.com/goreleaser/goreleaser/v2@${GORELEASER_VERSION}
name: I ... eleaserBuild release artifactsBuild r ... tifactsGITHUB_TOKEN${{ github.token }}GITHUB_ ... oken }}set -euo pipefail
attempts=4
for attempt in $(seq 1 "${attempts}"); do
  gh release delete "${GITHUB_REF_NAME}" --yes >/dev/null 2>&1 || true
  log_file="$(mktemp)"
  set +e
  goreleaser release --clean 2>&1 | tee "${log_file}"
  status=${PIPESTATUS[0]}
  set -e

  if [ "${status}" -eq 0 ]; then
    rm -f "${log_file}"
    exit 0
  fi

  if grep -q " 429 " "${log_file}" && [ "${attempt}" -lt "${attempts}" ]; then
    sleep_seconds=$((attempt * 60))
    echo "GoReleaser hit GitHub API throttling (attempt ${attempt}/${attempts}); retrying in ${sleep_seconds}s..."
    rm -f "${log_file}"
    sleep "${sleep_seconds}"
    continue
  fi

  rm -f "${log_file}"
  exit "${status}"
done
exit 1
