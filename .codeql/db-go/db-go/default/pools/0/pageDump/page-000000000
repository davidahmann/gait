go build -o ./gait ./cmd/gait
mkdir -p gait-out

if [[ ! -f fixtures/run_demo/runpack.zip || ! -f gait.yaml ]]; then
  ./gait demo
  ./gait regress init --from run_demo --json
fi

set +e
./gait regress run --json --junit=./gait-out/junit.xml > ./gait-out/regress_result.json
status=$?
set -e

if [[ "$status" -eq 0 ]]; then
  echo "regress pass"
elif [[ "$status" -eq 5 ]]; then
  echo "regress fail (stable exit code 5)"
  exit 5
else
  echo "unexpected regress exit code: $status"
  exit "$status"
fi

# Endpoint policy fixture checks.
./gait policy test examples/policy/endpoint/allow_safe_endpoints.yaml examples/policy/endpoint/fixtures/intent_allow.json --json
set +e
./gait policy test examples/policy/endpoint/block_denied_endpoints.yaml examples/policy/endpoint/fixtures/intent_block.json --json
block_status=$?
./gait policy test examples/policy/endpoint/require_approval_destructive.yaml examples/policy/endpoint/fixtures/intent_destructive.json --json
approval_status=$?
set -e
if [[ "$block_status" -ne 3 ]]; then
  echo "endpoint block fixture exit mismatch: $block_status"
  exit 1
fi
if [[ "$approval_status" -ne 4 ]]; then
  echo "endpoint approval fixture exit mismatch: $approval_status"
  exit 1
fi

# Skill provenance verification path.
bash scripts/test_skill_supply_chain.shUse this in non-GitHub providers (Jenkins, Buildkite, CircleCI, etc.):generic-shell-ci-snippet-compatibility-onlyGeneric Shell CI Snippet (Compatibility Only)Upload , and fixture artifacts.junit.xmlFail with stable exit codes ( for deterministic regression failure).Run skill provenance verification checks.Run endpoint policy fixture checks ().Run gait regress run --json --junit=...Restore fixture ( + ) or initialize from a run artifact.fixtures/run_demo/runpack.zipBuild local  binary.Template flow:Use  as the baseline workflow..github/workflows/adoption-regress-template.ymlgithub-actions-templateGitHub Actions TemplateCanonical default path:This kit makes incident-to-regression checks turnkey in CI.ci-regress-kit-epic-a51CI Regress Kit (Epic A5.1)https://davidahmann.github.io/gait/docs/ci_regress_kit/Gait documentation page: CI Regress Kit (Epic A5.1)CI Regress Kit (Epic A5.1) | Gait Docs/Users/davidahmann/Projects/gait/docs-site/.next/server/app/docs/concepts/mental_model.html/Users/davidahmann/Projects/gait/docs-site/.next/server/app/docs/conceptsself.__next_f.push([1,"a:{\"metadata\":[[\"$\",\"title\",\"0\",{\"children\":\"Gait Mental Model | Gait Docs\"}],[\"$\",\"meta\",\"1\",{\"name\":\"description\",\"content\":\"Gait documentation page: Gait Mental Model\"}],[\"$\",\"meta\",\"2\",{\"name\":\"keywords\",\"content\":\"agent control plane, agent safety, ai governance, runpack, policy gate, deterministic regression, prompt injection defense, ai incident response\"}],[\"$\",\"link\",\"3\",{\"rel\":\"canonical\",\"href\":\"https://davidahmann.github.io/gait/docs/concepts/mental_model/\"}],[\"$\",\"meta\",\"4\",{\"property\":\"og:title\",\"content\":\"Gait Mental Model | Gait Docs\"}],[\"$\",\"meta\",\"5\",{\"property\":\"og:description\",\"content\":\"Gait documentation page: Gait Mental Model\"}],[\"$\",\"meta\",\"6\",{\"property\":\"og:url\",\"content\":\"https://davidahmann.github.io/gait/docs/concepts/mental_model/\"}],[\